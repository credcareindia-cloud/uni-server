{
  "name": "uniqube-3d-backend",
  "version": "1.0.0",
  "description": "3D FRAG Viewer Platform Backend API",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "node --max-old-space-size=8192 --expose-gc --no-warnings ./node_modules/tsx/dist/cli.mjs watch src/index.ts",
    "build": "tsc --noEmit false --skipLibCheck --noUnusedLocals false --noUnusedParameters false",
    "build:soft": "tsc --noEmit false || true",
    "build:quiet": "tsc --noEmit false >/dev/null 2>&1 || true",
    "build:check": "eslint \"src/**/*.{ts,tsx,js}\" && tsc --project tsconfig.strict.json",
    "build:prod": "prisma generate && tsc --noEmit false --skipLibCheck --noUnusedLocals false --noUnusedParameters false",
    "build:prod:soft": "prisma generate && tsc --noEmit false || true",
    "build:prod:quiet": "prisma generate && tsc --noEmit false >/dev/null 2>&1 || true",
    "start": "node dist/index.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "make-admin": "tsx scripts/make-admin.ts",
    "db:seed": "tsx src/scripts/seed.ts",
    "lint": "eslint src --ext .ts",
    "test": "jest"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.478.0",
    "@aws-sdk/s3-request-presigner": "^3.478.0",
    "@prisma/client": "^5.7.1",
    "@thatopen/components": "^3.2.1",
    "@thatopen/fragments": "^3.2.0",
    "@thatopen/ui": "^3.2.0",
    "@types/multer": "^2.0.0",
    "@types/three": "^0.181.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.5.2",
    "express-rate-limit": "^7.1.5",
    "flatbuffers": "^25.9.23",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "three": "^0.180.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "jest": "^29.7.0",
    "open": "^10.2.0",
    "prisma": "^5.7.1",
    "puppeteer": "^24.27.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.46.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}